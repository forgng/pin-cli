{
  "name": "pin-cli",
  "description": "Pin your most used folders. Quickly teleport to pinned locations. Guarantee to save up to 10 seconds.",
  "version": "1.2.1",
  "author": "@forgng",
  "bin": {
    "pin": "./bin/run"
  },
  "bugs": "https://github.com/forgng/pin/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "chalk": "3.0.0",
    "enquirer": "2.3.2",
    "execa": "4.0.0",
    "tslib": "^1",
    "update-notifier": "4.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "np": "5.2.1",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/forgng/pin",
  "keywords": [
    "cli",
    "pin"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "np": {
    "test": false
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "pin",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": [
        "./lib/hooks/init/init",
        "./lib/hooks/init/checkUpdate"
      ]
    }
  },
  "repository": "forgng/pin",
  "scripts": {
    "test": "echo LOL",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "readme": "oclif-dev readme",
    "manifest": "oclif-dev manifest",
    "publish": "np"
  },
  "types": "lib/index.d.ts"
}
